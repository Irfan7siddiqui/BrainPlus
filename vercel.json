{
    "version": 2,
    "builds": [
      {
        "src": "brainplus/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "requirements.txt",
        "use": "@vercel/python"
      },
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt",
          "buildCommand": "python3 manage.py collectstatic --noinput"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "brainplus/wsgi.py"
      }
    ]
  }
  